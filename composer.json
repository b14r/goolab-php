{
    "name": "mntbllr/goolab-php",
    "description": "gooラボ Japanese Analasys API PHP Wrapper",
    "keywords": [
        "mntbllr",
        "goolab-php"
    ],
    "homepage": "https://github.com/mntbllr/gooLab-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Montibeller",
            "email": "bruno.e.montibeller@gmail.com",
            "homepage": "https://mntbllr.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mntbllr\\GooLab\\": "src"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
